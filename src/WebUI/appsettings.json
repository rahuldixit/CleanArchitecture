{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CaWorkshop;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
      "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      }
    },
  "IdentityServer": {
    "Clients": {
      "CaWorkshop.WebUI": {
        "Profile": "IdentityServerSPA"
      }
    },
    "HealthChecksUI": {
      "HealthChecks": [
        {
          "Name": "CaWorkshop",
          "Uri": "https://localhost:7021/health"
        }
      ],
      "EvaluationTimeInSeconds": 10,
      "MinimumSecondsBetweenFailureNotifications": 60
    }
  },
"AllowedHosts": "*"
}
